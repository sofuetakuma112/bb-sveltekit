{
  "name": "bb-sveltekit",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "npm run build && wrangler pages dev",
    "db:create": "wrangler d1 create dev-sveltekit-d1",
    "db:drop": "drizzle-kit drop",
    "db:generate": "drizzle-kit generate",
    "db:migrate:local": "wrangler d1 migrations apply dev-sveltekit-d1 --local",
    "db:migrate:remote": "wrangler d1 migrations apply dev-sveltekit-d1 --remote",
    "db:init:local": "wrangler d1 execute dev-sveltekit-d1 --local --file=./migrations/0000_calm_mach_iv.sql",
    "db:init:remote": "wrangler d1 execute dev-sveltekit-d1 --remote --file=./migrations/0000_calm_mach_iv.sql",
    "db:reset:local": "wrangler d1 execute dev-sveltekit-d1 --local --file=./reset.sql",
    "db:reset:remote": "wrangler d1 execute dev-sveltekit-d1 --remote --file=./reset.sql",
    "db:studio": "cross-env LOCAL_DB_PATH=$(find .wrangler/state/v3/d1/miniflare-D1DatabaseObject -type f -name '*.sqlite' -print -quit) drizzle-kit studio",
    "db:studio:prod": "drizzle-kit studio",
    "db:seed:local": "cross-env SEED_IMAGES=$(ls -1 ./seed/images | tr '\n' ',') LOCAL=true node ./seed/seed-runner.js",
    "db:seed:remote": "cross-env SEED_IMAGES=$(ls -1 ./seed/images | tr '\n' ',') LOCAL=false node ./seed/seed-runner.js",
    "db:seed:images:local": "cross-env LOCAL_R2_PATH=$(find .wrangler/state/v3/r2/miniflare-R2BucketObject -type f -name '*.sqlite' -print -quit) node ./seed/upload-images.js",
    "db:seed:images:remote": "node ./seed/upload-images-to-remote.js",
    "bucket:create": "wrangler r2 bucket create bb-sveltekit-dev",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "deploy": "npm run build && wrangler pages deploy",
    "cf-typegen": "wrangler types && mv worker-configuration.d.ts src/"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.600.0",
    "@cloudflare/workers-types": "^4.20240620.0",
    "@egoist/tailwindcss-icons": "^1.8.1",
    "@fontsource/fira-mono": "^5.0.13",
    "@iconify/json": "^2.2.221",
    "@neoconfetti/svelte": "^2.2.1",
    "@sveltejs/adapter-auto": "^3.2.2",
    "@sveltejs/adapter-cloudflare": "^4.5.0",
    "@sveltejs/kit": "^2.5.17",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@types/eslint": "^8.56.10",
    "@types/ua-parser-js": "^0.7.39",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "autoprefixer": "^10.4.19",
    "cfw-bindings-wrangler-bridge": "^0.11.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.22.7",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.40.0",
    "lucia": "^3.2.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.5",
    "sqlite3": "^5.1.7",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.1",
    "tailwindcss": "^3.4.4",
    "tslib": "^2.6.3",
    "typescript": "^5.5.2",
    "vite": "^5.3.1",
    "wrangler": "^3.61.0"
  },
  "type": "module",
  "dependencies": {
    "@cf-wasm/photon": "^0.1.20",
    "@faker-js/faker": "^8.4.1",
    "@lucia-auth/adapter-drizzle": "^1.0.7",
    "@lucia-auth/adapter-sqlite": "^3.0.1",
    "arctic": "^1.9.1",
    "better-sqlite3": "^9.6.0",
    "bits-ui": "^0.21.10",
    "clsx": "^2.1.1",
    "cmdk-sv": "^0.0.17",
    "drizzle-orm": "^0.31.2",
    "lucide-svelte": "^0.396.0",
    "mode-watcher": "^0.3.1",
    "sveltekit-superforms": "^2.15.1",
    "tailwind-merge": "^2.3.0",
    "tailwind-variants": "^0.2.1",
    "tailwindcss-animate": "^1.0.7",
    "ua-parser-js": "^1.0.38",
    "uuid": "^10.0.0"
  }
}
